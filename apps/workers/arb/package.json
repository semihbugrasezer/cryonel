{
  "name": "@cryonel/worker-arb",
  "version": "1.0.0",
  "description": "CRYONEL Arbitrage Worker - Cross-chain arbitrage detection and execution",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t cryonel-worker-arb .",
    "docker:run": "docker run --env-file .env cryonel-worker-arb"
  },
  "dependencies": {
    "ccxt": "^4.2.15",
    "@solana/web3.js": "^1.91.1",
    "@jup-ag/core": "^4.0.0-beta.23",
    "axios": "^1.7.2",
    "redis": "^4.6.13",
    "pg": "^8.13.1",
    "bullmq": "^5.1.1",
    "ioredis": "^5.3.2",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "ws": "^8.18.0",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/ws": "^8.5.10",
    "@types/pg": "^8.11.2",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.5.3",
    "tsx": "^4.19.2",
    "vitest": "^1.4.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}